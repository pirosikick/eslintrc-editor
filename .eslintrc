{
  "env": [
    "browser",
    "node",
    "mocha",
    "es6"
  ],
  "globals": {
    "React": true
  },
  "parser": "babel-parser",
  "rules": {
    "array-bracket-spacing": 2,
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "arrow-spacing": 2,
    "block-spacing": 2,
    "brace-style": 2,
    "callback-return": 2,
    "camelcase": 2,
    "comma-dangle": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "consistent-return": 2,
    "consistent-this": [
      2,
      "self"
    ],
    "constructor-super": 2,
    "curly": 2,
    "dot-location": [
      2,
      "property"
    ],
    "indent": [
      2,
      2
    ],
    "jsx-quotes": [
      2,
      "prefer-double"
    ],
    "key-spacing": 2,
    "max-depth": [
      2,
      3
    ],
    "max-len": 2,
    "max-nested-callbacks": 2,
    "new-parens": 2,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-cond-assign": 2,
    "no-console": 2,
    "no-constant-condition": 2,
    "no-debugger": 2,
    "no-div-regex": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-else-return": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-inline-comments": 2,
    "no-irregular-whitespace": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 2
  }
}
